<?xml version="1.0" encoding="UTF-8"?>
<?import io.github.palexdev.materialfx.controls.*?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="400" maxWidth="280" minHeight="400" minWidth="280" prefHeight="400" prefWidth="280"
      xmlns:fx="http://javafx.com/fxml" fx:controller="com.vak.oop.controller.ImportCreateController" spacing="10"
      alignment="CENTER" stylesheets="@OOP.css">
  <MFXComboBox fx:id="pdnameField" floatingText="Product Name" floatMode="BORDER" prefWidth="200" prefHeight="40"/>
  <MFXTextField fx:id="pdpriceField" floatingText="Product Price" floatMode="BORDER" prefWidth="200" prefHeight="40"/>
  <MFXTextField fx:id="pdquantityField" floatingText="Product Quantity" floatMode="BORDER" prefWidth="200"
                prefHeight="40"/>
  <MFXButton buttonType="RAISED" text="Submit" prefWidth="100" prefHeight="40" onAction="#handleSubmit"
             styleClass="Btn"/>
</VBox>